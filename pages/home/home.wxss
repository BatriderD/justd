.container {
  width: 100%;
  background-color: #f2f2f2;
}

.user-list {
  display: flex;
  flex-direction: column;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.user-item {
  width: 100%;
  align-items: center;
  display: -webkit-flex;
}
.user-name{
  margin-left: 10rpx;
}
.user-button{
  display: -webkit-flex;
  margin-left: auto;
  margin-right: 10rpx;
  padding: 10rpx;
}
.del{
  font-size: 20;
  color: yellow;
  margin-right: 10rpx;
}
.change{
  font-size: 20;
  color: red;
}
